{
  "name": "donprinton",
  "scripts": {
    "build": "yarn build:umd:min && yarn sass",
    "build:win": "yarn build:umd:min:win && yarn build:umd && yarn sass && yarn assets && yarn i18n",
    "build:umd": "webpack --display-modules && yarn assets",
    "build:umd:watch": "webpack --display-modules --progress --colors --watch",
    "build:umd:min": "NODE_ENV=production webpack && yarn assets",
    "build:umd:min:win": "SET NODE_ENV=production webpack",
    "test": "jest",
    "sass": "compass compile  --force --trace && yarn assets",
    "sass:watch": "compass watch  --force --trace",
    "dev": "yarn run build:umd:watch | yarn run sass:watch",
    "i18n": "php cli/locale.php",
    "assets": "php cli/assets.php",
    "release": "yarn build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "4.0.0-alpha.6",
    "html-loader": "^0.4.5",
    "pug-html-loader": "^1.1.5",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^2.4.1"
  },
  "dependencies": {
  }
}
